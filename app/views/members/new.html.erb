<div class="Background">
  <div class="About">

  <h2>Create Member</h2>

    <form action="/members"  method="POST">
      <center>
      <table border="0" cellpadding="10" class="form">
        <tr>
            <td>First Name:</td>
            <td><input class="input" type="text" name="f_name" required></td>
        </tr>
        <tr>
            <td>Last Name:</td>
            <td><input class="input" type="text" name="l_name" required></td>
        </tr>
        <tr>
            <td>Address:</td>
            <td><input class="input" type="text" name="address" required></td>
        </tr>
        <tr>
            <td>Phone Number (input as xxx-xxx-xxxx):</td>
            <td><input class="input" type="text" name="phone_number" required></td>
        </tr>
        <tr>
            <td>Section:</td>
            <td><input class="input" type="text" name="section" required></td>
        </tr>
        <tr>
            <td>Email:</td>
            <td><input class="input" type="email" name="email" required></td>
        </tr>
        <tr>
            <td>Password:</td>
            <td> <input class="input" type="password" name="password" required></td>
            <input name="authenticity_token" value="<%= form_authenticity_token() %>" type="hidden" />
        </tr>
      </table></br></br>
      <button>Submit</button>
      </center>
    </form>

  </div>
</div>
